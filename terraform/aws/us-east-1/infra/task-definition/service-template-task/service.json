[
    {
      "name": "${app-name}",
      "image": "590284024382.dkr.ecr.us-east-1.amazonaws.com/${app-name}:latest",
      "dockerLabels": {
        "app": "${app-name}.web.${env}"
      },
      "cpu": "${cpu}",
      "memory": "${mem}",
      "memoryReservation": "${memRes}",
      "essential": true,
      "environment": [
        {
          "name": "ENV",
          "value": "${env}"
        },
        {
          "name": "SERVICE_NAME",
          "value": "${app-name}"
        }
      ],
      "portMappings": [
        {
          "containerPort": "${containerPort}",
          "protocol": "tcp",
          "hostPort": 0
        }
      ],
      "logConfiguration": {
        "logDriver": "json-file"
      }
    }
]
